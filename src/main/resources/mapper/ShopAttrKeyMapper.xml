<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaosecond.shop.mapper.ShopAttrKeyMapper">

    <select id="getIdsByIdCategory" resultType="Integer">
        SELECT
            t.id
        FROM
            t_shop_attr_key t
        WHERE
            t.id_category = #{idCategory}
    </select>

</mapper>
